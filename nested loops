// For Loops....


package main

import "fmt"

func main() {

for i:=1; i<=6; i++ {  // Scope of I would be with in this for loop. Because the declaration of I happened in the loop.

	for j:=1 ; j<=6 ;j++ {

		if i == j {	
			fmt.Printf("%v \t" , j*j) 
			} else if i+j == 7 {
				fmt.Printf("%v \t" , j*j*j)
			  }	else { fmt.Printf("%v \t" , j) }
		}
	fmt.Printf("\n")
}

}
