package main

import ( "fmt"
		 "math/rand"
		 )
		 
func main() {

for i:=1; i<= 50; i++ {
	va := rand.Intn(200)
	switch {
		case va <10 :
			fmt.Printf("\n %v Its a single digit positive number" , va)
		case va >=10 && va <= 99 :
			fmt.Printf("\n %v Its a Double digit positive number" , va)
		}
	}
}
